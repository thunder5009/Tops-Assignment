#include <stdio.h>
int main() 
{
    int arr1[5] = {10, 20, 30, 40, 50};
    printf("\n1d Array elements:");
    for(i=0;i<5;i++) 
	{
        printf("%d",arr1[i]);
    }
    int arr2[3][3] = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };
    int sum;
    printf("\n2D Array elements:");
    for(row=0;row<3;row++) 
	{
        for(col=0;col<3;col++)
		{
            printf("%d ",arr2[row][col]);
            sum=arr2[row][col];
        }
    }
    printf("\nSum of all elements: %d  ", sum);
}
