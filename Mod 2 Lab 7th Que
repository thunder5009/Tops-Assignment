#include<stdio.h>
int main() 
{
    int i;
    printf("\nUsing break to stop at 5:  ");
    for(i=1;i<=10;i++) 
	{
       if(i==6) 
		{ 
           break;    
        }
        printf("%d",i);
    }
    printf("\nUsing continue to skip 3:  ");
    for(i=1;i<=10;i++) 
	{
        if(i==3)
     	{
           continue;
      	}
        printf("%d",i);
    }
}
